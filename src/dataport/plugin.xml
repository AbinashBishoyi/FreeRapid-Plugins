<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="dataport.cz" version="1.0.0" vendor="Vity"
        class="cz.vity.freerapid.plugins.services.dataport.DataPortServiceImpl">
    <attributes>
        <attribute id="urlRegex" value="http://(www\.)?dataport\.cz/.+"/>
        <attribute id="minver" value="0.85"/>
        <attribute id="maxver" value="0.899"/>
        <attribute id="services" value="dataport.cz"/>
        <!-- max count of parallel downloads, has to be set in ServiceImpl too -->
        <attribute id="maxDownloads" value="5"/>
        <attribute id="faviconImage" value="resources/favicon.png"/>
    </attributes>

    <runtime>
        <library id="dataport.cz" path="/" type="code"/>
    </runtime>
</plugin>