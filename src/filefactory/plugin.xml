<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="filefactory.com" version="1.1.12" vendor="Kajda+ntoskrnl"
        class="cz.vity.freerapid.plugins.services.filefactory.FileFactoryServiceImpl">
    <attributes>
        <attribute id="urlRegex" value="http://(www\.)?filefactory\.com/file/.+"/>
        <attribute id="minver" value="0.85"/>
        <attribute id="maxver" value="0.899"/>
        <attribute id="services" value="filefactory.com"/>
        <attribute id="faviconImage" value="resources/favicon.png"/>
    </attributes>
    <requires>
        <import plugin-id="recaptcha.com" plugin-version="1.0" match="greater-or-equal"/>
    </requires>
    <runtime>
        <library id="filefactory.com" path="/" type="code"/>
    </runtime>
</plugin>