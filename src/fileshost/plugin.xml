<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="fileshost.com" version="1.0.0" vendor="ntoskrnl"
        class="cz.vity.freerapid.plugins.services.fileshost.FilesHostServiceImpl">
    <attributes>
        <attribute id="urlRegex" value="http://(www\.)?fileshost\.com/.+"/>
        <attribute id="minver" value="0.85"/>
        <attribute id="maxver" value="0.899"/>
        <attribute id="services" value="fileshost.com"/>
        <attribute id="maxDownloads" value="4"/>
        <attribute id="faviconImage" value="resources/favicon.png"/>
    </attributes>

    <runtime>
        <library id="fileshost.com" path="/" type="code"/>
    </runtime>
</plugin>