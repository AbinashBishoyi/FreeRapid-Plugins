<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="filesmonster.com" version="1.0.4" vendor="Lukiz+ntoskrnl"
        class="cz.vity.freerapid.plugins.services.filesmonster.FilesMonsterServiceImpl">
    <attributes>
        <attribute id="urlRegex" value="http://(www\.)?filesmonster\.com/.+"/>
        <attribute id="minver" value="0.85"/>
        <attribute id="maxver" value="0.899"/>
        <attribute id="services" value="filesmonster.com"/>
        <attribute id="maxDownloads" value="1"/>
        <attribute id="faviconImage" value="resources/favicon.png"/>
    </attributes>
    <requires>
        <import plugin-id="recaptcha.com" plugin-version="1.0" match="greater-or-equal"/>
    </requires>
    <runtime>
        <library id="filesmonster.com" path="/" type="code"/>
    </runtime>
</plugin>