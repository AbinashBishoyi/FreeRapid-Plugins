<plugin class="cz.vity.freerapid.plugins.services.hellshare_full.HellshareServiceImpl" id="hellshare.com_full" vendor="Smisek+Jpexs" version="1.0.7">
    <attributes>
        <attribute id="urlRegex" value="http://(www\.)?hellshare\.com/.+|http://(download\.[a-z]+\.)?hellshare\.com/.+|http://download\.hellshare\.hu/.+|http://download\.hellshare\.sk/.+"/>
        <attribute id="minver" value="0.85"/>
        <attribute id="maxver" value="0.899"/>
        <attribute id="services" value="hellshare.com"/>
        <attribute id="maxDownloads" value="100"/>
        <!-- ID pluginu, pro ktere je to premium -->
        <attribute id="premium" value="true"/>
        <attribute id="hasOptions" value="true"/>
        <attribute id="faviconImage" value="resources/favicon.png"/>
    </attributes>

    <runtime>
        <library id="hellshare.com" path="/" type="code"/>
    </runtime>
</plugin>
