<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="hulu.com" version="1.0.4" vendor="ntoskrnl"
        class="cz.vity.freerapid.plugins.services.hulu.HuluServiceImpl">
    <attributes>
        <attribute id="urlRegex" value="http://(www\.)?hulu\.com/watch/.+"/>
        <attribute id="minver" value="0.85"/>
        <attribute id="maxver" value="0.899"/>
        <attribute id="services" value="hulu.com"/>
        <attribute id="maxDownloads" value="10"/>
        <attribute id="faviconImage" value="resources/favicon.png"/>
    </attributes>
    <requires>
        <import plugin-id="rtmp" plugin-version="2.0.0" match="greater-or-equal"/>
    </requires>
    <runtime>
        <library id="hulu.com" path="/" type="code"/>
    </runtime>
</plugin>