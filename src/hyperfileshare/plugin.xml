<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin class="cz.vity.freerapid.plugins.services.hyperfileshare.HyperFileShareServiceImpl" id="hyperfileshare.com" vendor="Vity" version="1.0.5">
    <attributes>
        <attribute id="urlRegex" value="http://(www\.|download\.)?hyperfileshare\.com/.+"/>
        <attribute id="minver" value="0.85"/>
        <attribute id="maxver" value="0.899"/>
        <attribute id="services" value="hyperfileshare.com"/>
        <!-- max count of parallel downloads, has to be set in ServiceImpl too -->
        <attribute id="maxDownloads" value="2"/>
        <attribute id="faviconImage" value="resources/favicon.png"/>
    </attributes>

    <runtime>
        <library id="hyperfileshare.com" path="/" type="code"/>
    </runtime>
</plugin>
