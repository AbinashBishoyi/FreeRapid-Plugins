<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="imagehaven.net" version="1.1" vendor="Arthur Gunawan+JPEXS"
        class="cz.vity.freerapid.plugins.services.imagehaven.ImagehavenServiceImpl">
    <attributes>
        <attribute id="urlRegex" value="http://(img|a)[0-9]+\.imagehaven\.net/img\.php\?.+"/>
        <attribute id="minver" value="0.85"/>
        <attribute id="maxver" value="0.899"/>
        <attribute id="services" value="imagehaven.net"/>
        <attribute id="maxDownloads" value="10"/>
        <attribute id="faviconImage" value="resources/favicon.png"/>
    </attributes>

    <runtime>
        <library id="imagehaven.net" path="/" type="code"/>
    </runtime>
</plugin>