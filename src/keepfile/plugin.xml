<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="keepfile.com" version="1.0" vendor="RickCL"
        class="cz.vity.freerapid.plugins.services.keepfile.KeepFileServiceImpl">
    <attributes>
        <attribute id="urlRegex" value="http://.*?keepfile\.com/.+"/>
        <attribute id="minver" value="0.85"/>
        <attribute id="maxver" value="0.899"/>
        <attribute id="services" value="keepfile.com"/>
        <attribute id="maxDownloads" value="1"/>
        <attribute id="faviconImage" value="resources/favicon.png"/>
    </attributes>
    <requires>
        <import exported="false" match="greater-or-equal" optional="false" plugin-id="recaptcha.com" plugin-version="1.0" reverse-lookup="false"/>
    </requires>
    <runtime>
        <library id="keepfile.com" path="/" type="code"/>
    </runtime>
</plugin>