<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin class="cz.vity.freerapid.plugins.services.linkdecrypter.LinkDecrypterServiceImpl"
        id="linkdecrypter.com" vendor="Vity" version="1.0.0">
    <attributes>
        <attribute id="urlRegex"
                   value="http://(www\.)?(linkcrypt.ws|klnk.de|link-protector.com|link-safe.net|linkzip.net)/.+"/>
        <attribute id="minver" value="0.851"/>
        <attribute id="maxver" value="0.899"/>
        <attribute id="services" value="linkcrypt.ws klnk.de link-protector.com link-safe.net linkx.in linkzip.net klnk.de"/>
        <attribute id="maxDownloads" value="10"/>
        <attribute id="faviconImage" value="resources/favicon.png"/>
    </attributes>

    <runtime>
        <library id="linkdecrypter.com" path="/" type="code"/>
    </runtime>
</plugin>