        <attribute id="urlRegex" value="(http://(www\.)?hellshare\.com/.+|http://(download\.[a-z]+\.)?hellshare\.com/.+|http://download\.hellshare\.hu/.+|http://download\.hellshare\.sk/.+)|(https?://.*?hotfile\.com/(dl|links)/.+)|(http://(www\.)?quickshare\.cz/.+)|(https?://((www\.)|(rs[0-9]+\.))?rapidshare\.com/files/[0-9]+/.*)|(http://(www\.)?(uloz\.to|ulozto\.net|ulozto\.cz|ulozto\.sk)/.+)"/>
