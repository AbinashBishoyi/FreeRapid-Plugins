<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="nova.cz" version="1.0.0" vendor="JPEXS"
        class="cz.vity.freerapid.plugins.services.nova.NovaServiceImpl">
    <attributes>
        <attribute id="urlRegex" value="http://archiv\.nova\.cz/multimedia/.+"/>
        <attribute id="minver" value="0.849"/>
        <attribute id="maxver" value="0.850"/>
        <attribute id="services" value="nova.cz"/>
        <!-- max count of parallel downloads, has to be set in ServiceImpl too -->
        <attribute id="maxDownloads" value="9"/>
        <attribute id="faviconImage" value="resources/favicon.png"/>
    </attributes>

    <runtime>
        <library id="nova.cz" path="/" type="code"/>
    </runtime>
</plugin>