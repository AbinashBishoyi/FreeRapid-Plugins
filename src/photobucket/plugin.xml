<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="photobucket.com" version="1.1.1" vendor="ntoskrnl"
        class="cz.vity.freerapid.plugins.services.photobucket.PhotoBucketServiceImpl">
    <attributes>
        <attribute id="urlRegex" value="http://([a-z]+?|[^i][a-z\d]+?)\.photobucket\.com/.+"/>
        <attribute id="minver" value="0.85"/>
        <attribute id="maxver" value="0.899"/>
        <attribute id="services" value="photobucket.com"/>
        <attribute id="maxDownloads" value="10"/>
        <attribute id="faviconImage" value="resources/favicon.png"/>
    </attributes>

    <runtime>
        <library id="photobucket.com" path="/" type="code"/>
    </runtime>
</plugin>