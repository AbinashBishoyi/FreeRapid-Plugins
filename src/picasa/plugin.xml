        <attribute id="urlRegex"
                   value="(https?://(www\.)?picasa.*?\.google.[a-z]{2,3}/.+?/.+)|(https?://.+?\.ggpht\.com/.+)"/>
        <attribute id="minver" value="0.852"/>
