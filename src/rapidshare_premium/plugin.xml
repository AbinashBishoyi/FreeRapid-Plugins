<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin class="cz.vity.freerapid.plugins.services.rapidshare_premium.RapidShareServiceImpl"
        id="RapidShare.com_premium" vendor="ATomSoft" version="1.3.0">
    <attributes>
        <attribute id="urlRegex" value="https?://((www\.)|(rs[0-9a-zA-Z]+\.))?rapidshare\.com/.+"/>
        <attribute id="minver" value="0.852"/>
        <attribute id="maxver" value="0.899"/>
        <attribute id="services" value="rapidshare.com"/>
        <attribute id="maxDownloads" value="100"/>
        <!-- ID pluginu, pro ktere je to premium -->
        <attribute id="premium" value="true"/>
        <attribute id="hasOptions" value="true"/>
        <attribute id="faviconImage" value="resources/favicon.png"/>
    </attributes>

    <runtime>
        <library id="rapidshare.com" path="/" type="code"/>
    </runtime>
</plugin>
