<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin class="cz.vity.freerapid.plugins.LibraryPlugin" id="recaptcha.com" vendor="Vity+Team" version="1.0.3">
    <attributes>
        <attribute id="minver" value="0.85"/>
        <attribute id="maxver" value="0.899"/>
        <attribute id="services" value="recaptcha.com"/>
        <!-- max count of parallel downloads, has to be set in ServiceImpl too -->
        <attribute id="maxDownloads" value="0"/>
        <!--<attribute id="faviconImage" value="resources/favicon.png"/>-->
    </attributes>

    <runtime>
        <library id="recaptcha.com" path="/" type="code">
            <export prefix="*"/>
        </library>
    </runtime>
</plugin>
