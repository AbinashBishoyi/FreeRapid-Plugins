<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="soundcloud.com" version="1.0.0" vendor="Vity"
        class="cz.vity.freerapid.plugins.services.soundcloud.SoundcloudServiceImpl">
    <attributes>
        <attribute id="urlRegex" value="http://(www\.)?soundcloud\.com/.+"/>
        <attribute id="minver" value="0.83"/>
        <attribute id="maxver" value="0.849"/>
        <attribute id="services" value="soundcloud.com"/>
        <!-- max count of parallel downloads, has to be set in ServiceImpl too -->
        <attribute id="maxDownloads" value="10"/>
        <attribute id="faviconImage" value="resources/favicon.png"/>
    </attributes>

    <runtime>
        <library id="soundcloud.com" path="/" type="code"/>
    </runtime>
</plugin>