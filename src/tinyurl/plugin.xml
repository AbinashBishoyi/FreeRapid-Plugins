<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="tinyurl.com" version="1.0" vendor="Alex"
        class="cz.vity.freerapid.plugins.services.tinyurl.TinyUrlServiceImpl">
    <attributes> 
        <attribute id="urlRegex" value="http://tinyurl.com/.+"/>
        <attribute id="minver" value="0.8"/>
        <attribute id="maxver" value="0.809"/>
        <attribute id="services" value="tinyurl.com"/>
        <attribute id="faviconImage" value="resources/favicon.gif"/>
    </attributes>

    <runtime>
        <library id="tinyurl.com" path="/" type="code"/>
    </runtime>
</plugin>