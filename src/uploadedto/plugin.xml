<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="uploaded.to" version="1.0.7" vendor="Vity+ntoskrnl"
        class="cz.vity.freerapid.plugins.services.uploadedto.UploadedToShareServiceImpl">
    <attributes>
        <attribute id="urlRegex" value="http://(www\.)?(uploaded|ul)\.to/.+"/>
        <attribute id="minver" value="0.85"/>
        <attribute id="maxver" value="0.899"/>
        <attribute id="services" value="uploaded.to"/>
        <attribute id="maxDownloads" value="1"/>
        <attribute id="faviconImage" value="resources/favicon.png"/>
    </attributes>
    <requires>
        <import plugin-id="recaptcha.com" plugin-version="1.0" match="greater-or-equal"/>
    </requires>
    <runtime>
        <library id="uploaded.to" path="/" type="code"/>
    </runtime>
</plugin>