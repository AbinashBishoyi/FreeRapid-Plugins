<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="uploadjocketredirect.com" version="1.0.0" vendor="Arthur Gunawan"
        class="cz.vity.freerapid.plugins.services.uploadjocketredirect.UploadJocketRedirectServiceImpl">
    <attributes>
        <attribute id="urlRegex" value="http://(www\.)?uploadjockey\.com/redirect\.php.+"/>
        <attribute id="minver" value="0.83"/>
        <attribute id="maxver" value="0.849"/>
        <attribute id="services" value="uploadjocketredirect.com"/>
        <!-- max count of parallel downloads, has to be set in ServiceImpl too -->
        <attribute id="maxDownloads" value="10"/>
        <attribute id="faviconImage" value="resources/favicon.png"/>
    </attributes>

    <runtime>
        <library id="uploadjocketredirect.com" path="/" type="code"/>
    </runtime>
</plugin>