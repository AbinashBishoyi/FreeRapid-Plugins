        <import exported="false" match="greater-or-equal" optional="false" plugin-id="mp4parser"
                plugin-version="1.0.1" reverse-lookup="false"/>
    </requires>
    <runtime>
        <library id="youtube.com" path="/" type="code"/>
    </runtime>
</plugin>
