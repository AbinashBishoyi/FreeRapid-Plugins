<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="zhlednito.cz" version="1.0.2" vendor="JPEXS"
        class="cz.vity.freerapid.plugins.services.zhlednito.ZhledniToServiceImpl">
    <attributes>
        <attribute id="urlRegex" value="http://(www\.)?zhlednito\.cz/w/.+"/>
        <attribute id="minver" value="0.85"/>
        <attribute id="maxver" value="0.899"/>
        <attribute id="services" value="zhlednito.cz"/>
        <!-- max count of parallel downloads, has to be set in ServiceImpl too -->
        <attribute id="maxDownloads" value="9"/>
        <attribute id="faviconImage" value="resources/favicon.png"/>
    </attributes>

    <runtime>
        <library id="zhlednito.cz" path="/" type="code"/>
    </runtime>
</plugin>